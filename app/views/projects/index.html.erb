<%- model_class = Project -%>

  <!-- Example row of columns -->
<div class="row">
 <% @projects.each do |project| %>
    <div class="col-md-4">
			<a href="<%=project_path(project)%>"><%= image_tag project.image.url, class: "img-responsive" %></a>
      <h2><a href="<%=project_path(project)%>"><%= project.name %> <%= project.beta? ? '- [Beta]' : '' %></a></h2>
    </div>
  <% end %>
</div>
